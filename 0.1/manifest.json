{
   "content_scripts": [ {
      "js": [ "content_script.js" ],
      "matches": [ "*://*/*" ],
      "run_at": "document_end"
   } ],
   "description": "Converts units in page automatically",
   "manifest_version": 2,
   "name": "UnitConvert",
   "version": "1.1.1",
   "browser_action": {
      "default_icon": "convert.png",
      "default_title": "UnitConvert!"
   }
}
